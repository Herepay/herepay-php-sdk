{
    "name": "herepay/herepay-php-sdk",
    "description": "Herepay payment gateway PHP SDK.",
    "version": "1.0.0",
    "type": "sdk",
    "authors": [
        {
            "name": "aleprosli",
            "email": "aliff.rosli96@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
        "ext-json": "*",
        "vlucas/phpdotenv": "^5.6@dev",
        "symfony/var-dumper": "7.3.x-dev",
        "illuminate/container": "11.x-dev"
    },
    "require-dev": {
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "psr/simple-cache": "^2.0",
        "phpunit/phpunit": "^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "HerepaySDK\\HerepayServiceProvider"
            ],
            "alias": {
                "Herepay": "HerepaySDK\\Facades\\Herepay"
            }
        }
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "HerepaySDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HerepaySDK\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    }
}
